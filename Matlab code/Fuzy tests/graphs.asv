function graphs(t, values, string)
    %Extract the values
    reference = values(:, 1, 1); %The same in all 
    pos = values(:, 2, :);
    angle = values(:, 3, :);
    %Graphs
    figure();
    subplot(2,1,1);plot(t, reference, 'r-', ...
                        t, pos(:, 1), 'b-', ...
                        t, pos(:, 2), 'p-', ...
                        t, pos(:, 3), 'k-', 'LineWidth', 1.5);
    xlabel('Time (s)');ylabel('Position (m)');grid();
    if string == 'm'
        legend('Ref', 'm = 5', 'm = 10', 'm = 2');
    else
        legend('Ref', 'm = 1', 'm = 3', 'm = 0.5');
    end
    subplot(2,1,2);plot(t, angle, 'c-', 'LineWidth', 2);
    xlabel('Time (s)');ylabel('Angle (rad)');grid();
    if string == 'm'
        legend('Ref', 'm = 5', 'm = 10', 'm = 2');
    else
        legend('Ref', 'm = 1', 'm = 3', 'm = 0.5');
    end
    sgtitle('Fuzzy controller');
end